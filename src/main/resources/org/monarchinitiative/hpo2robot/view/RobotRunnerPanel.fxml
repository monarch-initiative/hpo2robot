<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="newRobotItemBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
    <HBox maxHeight="25.0" prefHeight="25.0" prefWidth="1000.0">
        <Button fx:id="newRobotRowButton" prefWidth="150" text="Add new ROBOT item">
            <HBox.margin>
                <Insets bottom="2.0" left="10.0" right="10.0" top="5.0" />
            </HBox.margin>
        </Button>
      <Button fx:id="exportRobotButton" mnemonicParsing="false" text="Export ROBOT File">
         <HBox.margin>
            <Insets bottom="2.0" left="10.0" right="10.0" top="5.0" />
         </HBox.margin>
      </Button>
      <Button fx:id="clearRobotButton" mnemonicParsing="false" text="Clear ROBOT table">
         <HBox.margin>
            <Insets bottom="2.0" left="10.0" right="10.0" top="5.0" />
         </HBox.margin>
      </Button>
        <Button fx:id="clearRobotFileButton" mnemonicParsing="false" text="Clear ROBOT File">
        <HBox.margin>
            <Insets bottom="2.0" left="10.0" right="10.0" top="5.0" />
        </HBox.margin>
    </Button>
      <Button mnemonicParsing="false" fx:id="copyRobotCommandButton" text="Copy ROBOT Command">
         <HBox.margin>
            <Insets bottom="2.0" left="10.0" right="10.0" top="5.0" />
         </HBox.margin>
      </Button>
    </HBox>
    <HBox>
        <Label fx:id="robotStatusLabel">
            <HBox.margin>
                <Insets bottom="5.0" left="210.0" right="10.0" />
            </HBox.margin>
        </Label>
    </HBox>

</VBox>
